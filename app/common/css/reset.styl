img
  vertical-align top
body,p,ul,ol,li,h1,h2,h3,h4,h5,h6,input,button,dl,dd,dt,div
  margin 0
  padding 0
h1,h2,h3,h4,h5,h6
  font-weight normal
button,input[type=subimt],input[type=button],input[type="reset"]
  -webkit-appearance none
  border 0 none
  background-color transparent
  border-radius 0
button,input[type=subimt],input[type=button],input[type="reset"], select, a
  -webkit-tap-highlight-color rgba(0,0,0,0.4)
input, select, textarea, button
  outline 0
  font-size 100%
li, ul
  list-style none